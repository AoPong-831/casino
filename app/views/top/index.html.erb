<p>ランキング表示</p>
<table border = "1">
    <tr>
        <th>ユーザー名</th>
        <th>パスワード</th>
        <th>総資産</th>
        <th>借金</th>
        <th>来店回数</th>
        <th>最終来店日</th>
    </tr>
<% @users.each do |a| %>
        <tr>
            <th><%= a.name %></th>
            <th><%= a.pass %></th>
            <th><%= a.money %></th>
            <th><%= a.debt %></th>
            <th><%= a.visits %></th>
            <th><%= a.final_date %></th>
        </tr>
<% end %>
</table>